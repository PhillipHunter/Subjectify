{
    "name": "Subjectify",
    "author": "Phillip Hunter",
    "version": "0.0.1",
    "description": "Add selected subject text to the end of search engine queries",
    "options_page": "options.html",
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_popup": "options.html",
        "default_icon": "icon-96.png"
    },
    "permissions": ["storage"],
    "icons": {
        "48": "icon-48.png",
        "96": "icon-96.png"
    },
    "content_scripts": [
        {
        "matches": [
            "*://*.google.com/search*"
        ],
        "js": ["redirect_search.bundle.js"]
        }
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
